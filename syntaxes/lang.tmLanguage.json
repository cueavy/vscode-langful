{
	"name": "lang",
	"patterns": [
		{
			"match": "#.*",
			"name": "comment.line.lang"
		} , {
			"match": "([^#^=^\\s]+|)(\\s+=\\s+|\\s+=|=\\s+|=|)([^#^\\n]+|)",
			"captures": {
				"1": {
					"name": "string.lang"
				},
				"2": {
					"name": "keyword.lang"
				},
				"3": {
					"name": "string.lang"
				}
			}
		}
	],
	"scopeName": "source.lang"
}