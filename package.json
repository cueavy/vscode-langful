{
  "name" : "langful" ,
  "displayName" : "langful" ,
  "description" : "highlight .lang file" ,
  "version" : "0.0.7" ,
  "publisher" : "cueavyqwp" ,
  "engines" : {
    "vscode" : "^1.54.0"
  } ,
  "categories" : [ "Programming Languages" ] ,
  "repository": {
    "type": "git",
    "url": "https://github.com/cueavy/vscode-langful"
  } ,
  "license": "MIT",
  "contributes" : {
    "languages" : [ {
      "id" : "lang" ,
      "aliases" : [ "lang" , "lang" ] ,
      "extensions" : [ ".lang" ] ,
      "configuration" : "./language-configuration.json"
    } ] ,
    "grammars" : [ {
      "language" : "lang" ,
      "scopeName" : "source.lang" ,
      "path" : "./syntaxes/lang.tmLanguage.json"
    } ]
  }
}