{
	"name": "lang",
	"patterns": [
		{
			"match": "([^=]+)(=| =)([^#]+)",
			"captures": {
				"1": {
					"name": "string.lang"
				},
				"2": {
					"name": "keyword.lang"
				},
				"3": {
					"name": "string.lang"
				}
			}
		},
		{
			"match": "(#)(.*)",
			"captures": {
				"1": {
					"name": "comment.line.lang"
				},
				"2": {
					"name": "comment.line.lang"
				}
			}
		}
	],
	"scopeName": "source.lang"
}