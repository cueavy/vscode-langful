{
	"name" : "lang" ,
	"patterns" : [
		{
			"match" : "#.*" ,
			"name" : "comment.lang"
		} , {
			"match" : "([^#^=^\\s]+|)(\\s+=\\s+|\\s+=|=\\s+|=|)([^#]+|)" ,
			"captures" : {
				"1" : {
					"name" : "variable.lang"
				} ,
				"2" : {
					"name" : "keyword.lang"
				} ,
				"3" : {
					"name" : "string.lang"
				}
			}
		}
	] ,
	"scopeName" : "source.lang"
}